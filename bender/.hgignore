#use glob synatax
syntax: glob

#ignore Matlab build directory
*slprj*

#ignore Matlab library files compiled from C
*.mexw64
*.mexa64
*.mexmac*
*.mexw32

#ignore autosave files
*autosave*
*.orig
*.m~
*.swp
#ignore graph images in the main lander6 directory
*.png
*.fig

#ignore Simulink Coder generated stuff
*rt_rtw/*
wsim/*.log
csc_model_hz.h
wsim/csc_*/*
wsim/mtv/csc_*/*

#ignore codegen log files
codegen/codegen.log
codegen/codegen.log.bak

#ignore user-specific files (eg preferences)
wsim/wsim_prefs.m
wsim/common/patches/*.bak

#ignore autogenerated things
wsim/fsw/lc/csc_wpc_watchpoint_collection_lib.slx
wsim/fsw/lc/csc_apc_actionpoint_collection_lib.slx
wsim/fsw/sc/csc_rtc_rts_collection_lib.slx
wsim/common/bus_initialization/
puppetmaster_bin.h

#ignore scenario data
wsim/scenarios/scenario_*/*/*.csv
wsim/scenarios/scenario_*/*/*.mat
wsim/scenarios/scenario_*/*/*.txt
wsim/scenarios/scenario_*/*.zip
wsim/scenarios/scenario_*/*.tar.*

#ignore s-function generated things
kcv/sfun_hmg.*
kfd/sfun_orb.*
kir/sfun_tcp.*

#ignore unneeded VxWorks kernel project stuff
vxworks/kernel/*/Makefile*
vxworks/kernel/*/default/*
vxworks/kernel/*/compObjs.lst
vxworks/kernel/*/prjObjs.lst
vxworks/kernel/*/linkSyms.c
vxworks/kernel/*/prjComps.h
vxworks/kernel/*/prjConfig.c
vxworks/kernel/*/prjParams.h
vxworks/ramboot/*/Makefile*
vxworks/ramboot/*/default/*
vxworks/ramboot/*/compObjs.lst
vxworks/ramboot/*/prjObjs.lst
vxworks/ramboot/*/linkSyms.c
vxworks/ramboot/*/prjComps.h
vxworks/ramboot/*/prjConfig.c
vxworks/ramboot/*/prjParams.h
vxworks/vsb/*/test
vxworks/vsb/*/tags
vxworks/vsb/*/arm
vxworks/vsb/*/vsbClean.*
vxworks/vsb/*/h/arm
vxworks/vsb/*/h/config/vxConfig
vxworks/vsb/*/.coveragescope
content.romfs

#ignore compiled stuff
*.so
*.o
*.bin
*.lis
*.tbl
ctdt.*
bootrom_stage1.desc
tools/cmdUtil/cmdUtil
build/*/elf2cfetbl/elf2cfetbl
tools/pasm/pasm

#ignore cFE stuff
.cdskeyfile
.reservedkeyfile
.resetkeyfile

